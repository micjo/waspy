[Unit]
Description=Hive - runs erd and rbs experiments
After=wait_eth_up.service
Requires=wait_eth_up.service

[Service]
Type=simple
User=root
WorkingDirectory=/root/waspy/projects/hive
ExecStart=/root/waspy/projects/hive/run_hive.sh
Restart=on-failure
SyslogIdentifier=hive

[Install]
WantedBy=multi-user.target


